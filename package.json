  "devDependencies": {
    "@myrotvorets/eslint-config-myrotvorets-ts": "^3.0.0",
    "@types/express": "^5.0.0",
    "@types/supertest": "^7.0.0",
    "c8": "^11.0.0",
    "eslint-formatter-gha": "^2.0.0",
    "express": "^5.0.0",
    "express-openapi-validator": "^5.3.5",
    "node-reporter-gha": "^2.0.4",
    "node-reporter-sonarqube": "^1.0.1",
    "supertest": "^7.0.0",
